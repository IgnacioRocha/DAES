@model DAES.Model.SistemaIntegrado.Organizacion

<table class="table table-hover table-striped ">

   
    @for (var i = 0; i < Model.Saneamientos.Count(); i++)
    {
    
        @Html.HiddenFor(model => model.Saneamientos[i].IdSaneamiento)
        @Html.HiddenFor(model => model.Saneamientos[i].OrganizacionId)
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => Model.Saneamientos[i].FechaEscrituraPublicaa)
            @Html.EditorFor(model => Model.Saneamientos[i].FechaEscrituraPublicaa, new { htmlAttributes = new { @class = "form-control", @style = "width: 190px;" } })
        </th>
        <th>
            @Html.DisplayNameFor(model => Model.Saneamientos[i].FechaaPublicacionDiario)
            @Html.EditorFor(model => Model.Saneamientos[i].FechaaPublicacionDiario, new { htmlAttributes = new { @class = "form-control", @style = "width: 190px;" } })
        </th>
    </tr>
    <tr>
        <th colspan="2">
            @Html.DisplayNameFor(model => Model.Saneamientos[i].DatoGeneralesNotario)
            @Html.EditorFor(model => Model.Saneamientos[i].DatoGeneralesNotario, new { htmlAttributes = new { @class = "form-control", @style = "max-width: 1200px;" } })
        </th>
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => Model.Saneamientos[i].Fojass)
            @Html.EditorFor(model => Model.Saneamientos[i].Fojass, new { htmlAttributes = new { @class = "form-control", @style = "width: 190px;" } })
        </th>
        <th>
            @Html.DisplayNameFor(model => Model.Saneamientos[i].FechaaInscripcion)
            @Html.EditorFor(model => Model.Saneamientos[i].FechaaInscripcion, new { htmlAttributes = new { @class = "form-control", @style = "width: 190px;" } })
        </th>
    </tr>
    <tr>
        <th colspan="2">
            @Html.DisplayNameFor(model => Model.Saneamientos[i].DatossCBR)
            @Html.EditorFor(model => Model.Saneamientos[i].DatossCBR, new { htmlAttributes = new { @class = "form-control", @style = "max-width: 1200px;" } })
        </th>
    </tr>
    
    <tr>
        @Ajax.ActionLink("Eliminar", "SaneamientoDelete", null, new { Model.Saneamientos.ToArray()[i].IdSaneamiento, Model.Saneamientos.ToArray()[i].OrganizacionId }, new AjaxOptions { UpdateTargetId = "saneamiento" }, new { @style = "Text-Decoration : none" })
    </tr>
    
</thead>

    }

    <tbody>
        
    </tbody>
</table>


