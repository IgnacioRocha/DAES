@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => Model.NumeroRegistro)



    <div class="panel panel-default">
        <div class="panel-heading">Directorio</div>
        <div class="panel-body">
            <div id="directorio">
                @Html.Partial("_DirectorioEdit", Model)
            </div>
            @Ajax.ActionLink("Agregar directorio", "DirectorioAdd", null, new { Model.OrganizacionId }, new AjaxOptions { UpdateTargetId = "directorio" })
        </div>
    </div>
    if (Model.TipoOrganizacionId == 1)
    {
        <br />
        <div id="DivConsti">
            <label>Constitución posterior al 2003</label><br />
            <input id="Constitucion1" name="Constitucion" type="radio" onclick="PresentarViewPost()" /> si
            <input id="Constitucion2" name="Constitucion" type="radio" onclick="PresentarViewAnt()" /> no
        </div>
    }

    <div @*name="divExis"*@>
        @if (Model.TipoOrganizacionId == 1)
        {


            <div class="panel panel-default" id="divExis" style="display:none">
                <div class="panel-heading">Autorización de Existencia Legal</div>
                <div class="panel-body">
                    <div id="existenciaAnterior">
                        @Html.Partial("_ExistenciaAnterior", Model)
                    </div>
                    <br />
                    <input type="button" value="Limpiar campos" id="limpiar" class="btn btn-primary" />
                </div>
            </div>

            <div class="panel panel-default" id="CPostRPost" name="CPostRPost" style="display:none">
                <div class="panel-heading">Autorización de Existencia Legal</div>
                <div class="panel-body">
                    <div id="existencia">
                        @Html.Partial("_ModificacionCPostRPostEdit", Model)
                    </div>
                    <br />
                    <input type="button" value="Limpiar campos" id="limpiarExiPost" class="btn btn-primary" />
                </div>
            </div>
        }
        @if (Model.TipoOrganizacionId == 2 || Model.TipoOrganizacionId == 3)
        {

            <div class="panel panel-default">
                <div class="panel-heading">Autorización de Existencia Legal</div>
                <div class="panel-body">
                    <div id="existencia">
                        @Html.Partial("_ModificacionAG", Model)
                    </div>
                </div>
            </div>
        }
    </div>



    <div>
        @if (Model.TipoOrganizacionId != 2 && Model.TipoOrganizacionId != 3)
        {
            <label>Tiene Saneamiento?</label>
            <input id="Tiene" name="Tiene" type="checkbox">
            <hr>
            <div class="panel panel-default" id="divSane" name="divSane" style="display:none">
                <div class="panel-heading">Saneamiento</div>
                <div id="Saneamiento">
                    @Html.Partial("_SaneamientoEdit", Model)
                </div>
                <br />
                <input type="button" value="Limpiar campos" id="limpiarSan" class="btn btn-primary" />
            </div>
        }
    </div>

    if (Model.TipoOrganizacionId == 1)
    {
        <label>Reformas posterior al 2003</label>
        <br /><div id="DivRef">
            <input id="FReforma1" name="FReformaa" type="radio" onclick="PresentarView()" /> si
            <input id="FReforma2" name="FReformaa" type="radio" onclick="PresentarViewAnterior()" /> no
        </div>
    }

    if (Model.TipoOrganizacionId == 1)
    {


        <div class="panel panel-default" id="reformaDiv" style="display:none">
            <div class="panel-heading">Reforma</div>
            <div class="panel-body">
                <div id="reforma">
                    @Html.Partial("_Reforma", Model)
                </div>
                @Ajax.ActionLink("Agregar reforma", "ReformaAdd", null, new { Model.OrganizacionId }, new AjaxOptions { UpdateTargetId = "reforma" })
            </div>
        </div>

        <div class="panel panel-default" id="reformaDivPost" style="display:none">
            <div class="panel-heading">Reforma</div>
            <div class="panel-body">
                <div id="reformaPost">
                    @Html.Partial("_ReformaPost", Model)
                </div>

                @Ajax.ActionLink("Agregar reforma", "ReformaAddPost", null, new { Model.OrganizacionId }, new AjaxOptions { UpdateTargetId = "reformaPost" })
            </div>
        </div>
    }

    if (Model.TipoOrganizacionId == 3 || Model.TipoOrganizacionId == 2)
    {
        <div class="panel panel-default" id="reformaDivPostt">
            <div class="panel-heading">Reforma</div>
            <div class="panel-body">
                <div id="reformaAGAC">
                    @Html.Partial("_ReformaAGAC", Model)
                </div>

                @Ajax.ActionLink("Agregar reforma", "ReformaAddAGAC", null, new { Model.OrganizacionId }, new AjaxOptions { UpdateTargetId = "reformaAGAC" })
            </div>
        </div>


    }
    <div class="panel panel-default">
        <div class="panel-heading">Disolución</div>
        <div class="panel-body">
            <div id="disolucion">
                @Html.Partial("_DisolucionEdit", Model)
            </div>
        </div>
    </div>

    <input type="submit" value="Guardar" class="btn btn-primary my-3" />
    <p></p>

}

