<script type="text/javascript">

    $("#Tiene").change(function () {
        if ($("#Tiene").is(':checked')) {
            $("#divSane").show();
        } else {
            $("#divSane").hide();
        }
    });



    function actualizarRefA() {
        $("#reformaDiv").show();
        $("#FReforma1").prop("checked", false);
        $("#FReforma2").prop("checked", true);
        $("#reformaDivPost").hide();

    }

    function actualizarRefP() {
        $("#reformaDiv").hide();
        $("#FReforma1").prop("checked", true);
        $("#FReforma2").prop("checked", false);
        $("#reformaDivPost").show();

    }

    function actualizarExis() {
        $("#divExis").show();
        $("#Constitucion1").prop("checked", false);
        $("#Constitucion2").prop("checked", true);
        $("#CPostRPost").hide();
    }

    function actualizarExisPost() {
        $("#divExis").hide();
        $("#Constitucion1").prop("checked", true);
        $("#Constitucion2").prop("checked", false);
        $("#CPostRPost").show();
    }



    function LimpiarCampos() {

        document.getElementById("ExistenciaLegals_0__AutorizadoPor").value = "";
        document.getElementById("ExistenciaLegals_0__FechaPublicacionn").value = "";
        document.getElementById("ExistenciaLegals_0__FechaNorma").value = "";
        document.getElementById("ExistenciaLegals_0__NumeroNorma").value = "";
        document.getElementById("ExistenciaLegals_0__TipoNormaId").value = "";
    }

    function LimpiarCampoExisPosterior() {

        document.getElementById("ExistenciaPosteriors_0__FechaConstitutivaSocios").value = "";
        document.getElementById("ExistenciaPosteriors_0__FechaPublicacionn").value = "";
        document.getElementById("ExistenciaPosteriors_0__FechaEscrituraPublica").value = "";
        document.getElementById("ExistenciaPosteriors_0__DatosGeneralesNotario").value = "";
        document.getElementById("ExistenciaPosteriors_0__Fojas").value = "";
        document.getElementById("ExistenciaPosteriors_0__AnoInscripcion").value = "";
        document.getElementById("ExistenciaPosteriors_0__DatosCBR").value = "";
    }

    function LimpiarCamposSanemiento() {

        document.getElementById("Saneamientos_0__FechaEscrituraPublicaa").value = "";
        document.getElementById("Saneamientos_0__FechaaPublicacionDiario").value = "";
        document.getElementById("Saneamientos_0__DatoGeneralesNotario").value = "";
