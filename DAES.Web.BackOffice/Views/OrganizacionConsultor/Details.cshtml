@model DAES.Model.SistemaIntegrado.Organizacion
<h2>@Model.RazonSocial</h2>

<p>
    <a href="javascript:void(0);" onclick="history.go(-1);">Volver a la lista</a>
</p>

@Html.Partial("_OrganizacionDetails", Model)
@Html.Partial("_OrganizacionDocumento", Model.DocumentoSinContenidos)
@Html.Partial("_OrganizacionProceso", Model.Procesos)

@if (Model.ExistenciaLegals.Any())
{
@Html.Partial("_OrganizacionModificacion", Model.ExistenciaLegals)}

@if (Model.ExistenciaAnteriors.Any())
{
@Html.Partial("_OrganizacionModificacionAnterior", Model.ExistenciaAnteriors)}

@if (Model.ExistenciaPosteriors.Any())
{
@Html.Partial("_OrganizacionModificacionPosterior", Model.ExistenciaPosteriors)}


@Html.Partial("_OrganizacionSaneamiento", Model.Saneamientos)

@if (Model.ReformaPosteriors.Any())
{
@Html.Partial("_OrganizacionReformaPost", Model.ReformaPosteriors)}

@if (Model.ReformaAnteriors.Any())
{
@Html.Partial("_OrganizacionReformaAnterior", Model.ReformaAnteriors)}



@if (Model.Reformas.Any())
{
    @Html.Partial("_OrganizacionReforma", Model.Reformas)}

@Html.Partial("_OrganizacionDisolucion", Model.Disolucions)

<p>
    <a href="javascript:void(0);" onclick="history.go(-1);">Volver a la lista</a>
</p>