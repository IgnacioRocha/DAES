<script type="text/javascript">

    $(document).ready(function () {
        $('[data-toggle="collapse"]').click(function () {
            $('.collapse.in').collapse('hide')
        });
    })

    function CopyToClipboard(campo) {
        document.getElementById(campo).select();
        document.execCommand('copy');

        modificarIcono(campo);
    }

    function modificarIcono(campo) {
        //Se modifica el texto
        var copyText = document.getElementById(campo);
        copyText.style.borderColor = "green";

        //Se modifica el icono
        var icono = document.getElementById('T' + campo);
        icono.setAttribute('class', 'bi bi-clipboard-check');

        //Modificar Tooltip
        var tooltip = document.getElementById('T' + campo);
        tooltip.setAttribute('title', 'Copiado!');

        setTimeout(function () { resetIcono(campo); }, 1500);
    }

    function resetIcono(campo) {
        //se reestablece el texto
        var copyText = document.getElementById(campo);
        copyText.style.borderColor = null;

        //se reestablece el icono
        var icono = document.getElementById('T' + campo);
        icono.setAttribute('class', 'bi bi-clipboard');

        //se reestablece tooltip
        var tooltip = document.getElementById('T' + campo);
        tooltip.setAttribute('title', 'Copiame!');
    }

</script>


