@model DAES.Model.SistemaIntegrado.ConfiguracionCertificado

@{
    ViewBag.Title = "Editar";
    ViewBag.SubTitle = "Configuración certificado";
}

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">

<style type="text/css">
    .collapse .badge-secondary {
        cursor: pointer;
    }

    .collapse h4 {
        margin-left: 5px;
    }

    textarea {
        max-width: none !important;
        resize: none;
        min-height: 150px;
    }

    .collapse td input[type="text"] {
        background-color: white;
        font-size: 12.5px;
    }

    .collapse td .bi {
        cursor: pointer;
    }

    .multi-collapse {
        padding-left: 126px;
    }

    .bi-clipboard {
        height: 32px !important;
        width: 32px !important;
    }
</style>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.ConfiguracionCertificadoId)
    @Html.HiddenFor(model => model.TipoDocumentoId)
    @Html.HiddenFor(model => model.TipoOrganizacionId)

    <div class="alert alert-warning" role="alert">
        <strong>Importante: </strong> La previsualización de cada certificado no incluirá folio ni pie de firma con verificación pero una vez emitidos sí.
    </div>

    <div class="row">
        <div class="form-group">
            @Html.Label("Tipo de Documento:", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("TipoDocumentoId", null, htmlAttributes: new { @class = "form-control", @disabled = "disabled" })
                @Html.ValidationMessageFor(model => model.TipoDocumentoId, "", new { @class = "text-danger" })
            </div>
        </div>

        <br />
        <br />

        <div class="form-group">
            @Html.Label("Tipo de Organízación:", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("TipoOrganizacionId", null, htmlAttributes: new { @class = "form-control", @disabled = "disabled" })
                @Html.ValidationMessageFor(model => model.TipoOrganizacionId, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>

    <br />
    <div class="form-horizontal">
        <div>
            <p style="text-align: center">
                @{

                    //#Certificado de DISOLUCION
                    if (Model.TipoDocumentoId == (int)DAES.Infrastructure.Enum.TipoDocumento.CertificadoDisolucionTest)
                    {
                        //DISOLUCION PARA COOPERATIVAS
                        if (Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.Cooperativa)
                        {
                            <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseDisolcion" aria-expanded="false" aria-controls="multiCollapseExample1" onclick="Ajustar()">Datos Disolución (Párrafo 1)</a>
                        }

                        //DISOLUCION PARA AG y AC
                        if (Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.AsociacionConsumidores ||
                            Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.AsociacionGremial)
                        {
                            <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseDisolucionGenerico" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Párrafo 1</a>
                        }
                    }

                    //#Certificado de VIGENCIA
                    if (Model.TipoDocumentoId == (int)DAES.Infrastructure.Enum.TipoDocumento.Vigencia)
                    {
                        //Dado que las 3 tipos de Organizaciones (COOP - AG - AC) usan el mismo parrafo 1, se dejará solo ese
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseVigenciaG" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Párrafo 1</a>
                    }

                    //#Certificado de VIGENCIA DE ESTATUTOS
                    if (Model.TipoDocumentoId == (int)DAES.Infrastructure.Enum.TipoDocumento.VigenciaEstatutos &&
                        (Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.Cooperativa))
                    {
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOrganizacionV" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Organización (Párrafo 1)</a>
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseExistencia" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Existencia (Párrafo 2)</a>
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseSaneamiento" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Saneamiento (Párrafo 3)</a>
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseReformas" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Reformas (Párrafo 4)</a>
                    }

                    //#Certificado de VIGENCIA DE ESTATUTOS (Para AC)
                    if (Model.TipoDocumentoId == (int)DAES.Infrastructure.Enum.TipoDocumento.VigenciaEstatutos &&
                        (Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.AsociacionConsumidores ||
                         Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.AsociacionGremial))
                    {
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseVigenciaG" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Párrafo 1</a>
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseVigenciaX" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Vigencia (Párrafo 2)</a>
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseModificacionEstatuaria" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Modificación Estatuaria (Párrafo 4)</a>
                    }

                    //#Certificado de VIGENCIA Y DIRECTORIO
                    if (Model.TipoDocumentoId == (int)DAES.Infrastructure.Enum.TipoDocumento.VigenciaDirectorio &&
                        (Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.AsociacionConsumidores ||
                         Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.AsociacionGremial ||
                         Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.Cooperativa))
                    {
                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseVDACAGCOOP" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Párrafo 1</a>
                    }

                    if (Model.TipoDocumentoId == (int)DAES.Infrastructure.Enum.TipoDocumento.VigenciaDirectorio &&
                             Model.TipoOrganizacionId == (int)DAES.Infrastructure.Enum.TipoOrganizacion.Cooperativa)
                    {

                        <a class="btn btn-primary" href="!#" type="button" data-bs-toggle="collapse" data-bs-target="#collapseVDParrafo2" aria-expanded="false" aria-controls="multiCollapseExample1">Configuración Párrafo 2</a>
                    }

                }

