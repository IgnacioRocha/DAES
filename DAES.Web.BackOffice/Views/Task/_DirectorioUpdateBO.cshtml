@model DAES.Web.BackOffice.Controllers.TaskModel

<table table table-hover table-striped datatable>
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Directorios.FirstOrDefault().NombreCompleto)</th>
            <th>@Html.DisplayNameFor(model => model.Directorios.FirstOrDefault().Rut)</th>
            <th>@Html.DisplayNameFor(model => model.Directorios.FirstOrDefault().Cargo.Nombre)</th>
            <th>@Html.DisplayNameFor(model => model.Directorios.FirstOrDefault().Genero.Nombre)</th>
            <th>@Html.DisplayNameFor(model => model.Directorios.FirstOrDefault().FechaInicio)</th>
            <th>@Html.DisplayNameFor(model => model.Directorios.FirstOrDefault().FechaTermino)</th>
        </tr>
    </thead>
    <tbody>


        @for (int i = 0; i < Model.ActualizacionOrganizacion.Directorio.Count(); i++)
        {
            <tr>
                <td>@Html.EditorFor(m => m.Directorios[i].NombreCompleto)</td>
                <td>@Html.EditorFor(m => m.Directorios[i].Rut)</td>
                <td>@Html.EditorFor(m => m.Directorios[i].Cargo.Nombre)</td>
                <td>@Html.EditorFor(m => m.Directorios[i].Genero.Nombre)</td>
                <td>@Html.EditorFor(m => m.Directorios[i].FechaInicio)</td>
                <td>@Html.EditorFor(m => m.Directorios[i].FechaTermino)</td>
            </tr>

            <tr>@Html.EditorFor(m => m.Directorios[i])</tr>
        }
    </tbody>
</table>
