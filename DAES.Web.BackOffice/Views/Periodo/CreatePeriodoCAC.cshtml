@model DAES.Model.SistemaIntegrado.PeriodoCAC
@{
    ViewBag.Title = "CreatePeriodoCAC";
}

<h2>Crear nuevo Periodo para supervisión CAC</h2>

@using (Html.BeginForm("CreatePeriodoCAC", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        @Html.LabelFor(q => q.Descripcion)
        @Html.EditorFor(q => q.Descripcion)
    </div>

    <button type="submit" class="btn btn-success">Agregar</button>
}